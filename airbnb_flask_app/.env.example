# Example environment file for the Flask Airbnb clone.
# Copy this file to `.env` and fill in your own credentials.

# Supabase credentials
SUPABASE_URL="https://YOUR_SUPABASE_URL.supabase.co"
SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"
SUPABASE_SERVICE_ROLE="YOUR_SUPABASE_SERVICE_ROLE"

# Google Maps API key
GOOGLE_MAPS_API_KEY="YOUR_GOOGLE_MAPS_API_KEY"

# Flask secret key (used for session cookies)
SECRET_KEY="CHANGE_ME_TO_A_RANDOM_SECRET"

# Optional: URL used in the magic link email to redirect back to your app.
# It should be a publicly accessible endpoint (e.g. via ngrok during development).
MAGIC_LINK_REDIRECT_URL="http://localhost:5000/auth/callback"
